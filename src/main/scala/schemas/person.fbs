namespace MyNamespace;

table People {
    people: [Person];
}

table Person {
    id: string;
    firstName: string;
    lastName: string;
    age: int;
    gender: string;
    email: string;
    phone: string;
    address: Address;
}

table Address {
    street: string;
    city: string;
    state: string;
    postalCode: string;
    country: string;
}

root_type People;
